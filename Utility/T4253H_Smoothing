# Environment
import pandas as pd

# TEMP for testing
vector = [1, 2, 3, 4, 5, 6, 7, 7.7, 6, 8, 9]


def running_median(array, window):
    s = pd.Series(array)
    output = s.rolling(window).median()
    return output


def t4253h_smoothing(array):
    s = pd.Series(array)
    i = s.rolling(4).median()
    i = i.rolling(2).median()
    i = i.rolling(5).median()
    i = i.rolling(3).median()
    return i
