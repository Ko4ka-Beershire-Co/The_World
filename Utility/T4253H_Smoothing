# Environment

import numpy as np
import scipy as sp
import scipy.ndimage

# TEMP for testing
vector = [1, 2, 3, 4, 5, 6, 7, 7.7, 6, 8, 9]


# T4253H
def running_median(array, mode='reflect'):
    output = sp.ndimage.median_filter(array, len(array), mode=mode)
    return output


print(running_median(vector))
